{
  "name": "devsupport/devsupport-instamojo-php",
  "type": "library",
  "description": "This is composer wrapper for instamojo v2 api for creating payment and handling notify and redirect calls",
  "license": "MIT",
  "homepage": "https://github.com/devsupport/devsupport-instamojo-php",
  "keywords": [
    "instamojo",
    "php",
    "api",
    "wrapper"
  ],
  "authors": [
    {
      "name": "Parth Mudgal",
      "email": "parth@devsupport.ai",
      "homepage": "https://github.com/artpar/devsupport-instamojo-php"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "autoload": {
    "classmap": [
      "src",
      "tests"
    ],
    "psr-4": {
      "Devsupport\\Client\\": "src/",
      "Devsupport\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/artpar/devsupport-instamojo-php/issues",
    "docs": "https://github.com/artpar/devsupport-instamojo-php/",
    "email": "support@devsupport.ai"
  }
}
